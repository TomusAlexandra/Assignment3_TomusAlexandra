@model StudentsApi.ModelsUse.LoginUser
@{
    ViewBag.Title = "Log in";
}

<h2 >Login </h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <img src="https://www.coralcalcium.com/assets/i/doctor2.png" alt="" class="app__meeting-photo" />
        <img src="https://www.findadoctor.com/fad/App_Themes/FADTheme/IndexImages/ML_SIMG.jpg" alt="" class="app__meeting-photo" />

        

        <div style="width: initial; background-color:bisque;"> </div>
        
        <fieldset>
           
               <li>@Html.LabelFor(m => m.UserName)
                   @Html.TextBoxFor(m => m.UserName)
                   @Html.ValidationMessageFor(m => m.UserName)
                
               </li>
                   
                
               <li>
                 @Html.LabelFor(m => m.Password)
                 @Html.PasswordFor(m => m.Password)
                 @Html.ValidationMessageFor(m => m.Password)
              </li>
       
           
         
           <input  type="submit" value="LogIn" />
       
           
        </fieldset>
     
       
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
